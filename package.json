{
  "name": "imdb-visualizer",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "apexcharts": "^3.5.1",
    "autoprefixer": "^9.7.0",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^6.1.3",
    "react": "^16.8.4",
    "react-apexcharts": "^1.3.0",
    "react-dom": "^16.8.4",
    "react-router-dom": "^4.3.1",
    "react-scripts": "2.1.5",
    "react-table": "^6.9.2"
  },
  "scripts": {
    "start": "npm-run-all --parallel watch:css start:react",
    "build": "npm-run-all build:css build:react",
    "build:css": "postcss src/styles/tailwind.css -o src/styles/index.css",
    "watch:css": "postcss src/styles/tailwind.css -o src/styles/index.css -w",
    "start:react": "react-scripts start",
    "build:react": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "eslint src",
    "deploy": "gh-pages -d build"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    "last 3 version",
    "not dead",
    "not ie > 0"
  ],
  "devDependencies": {
    "@tailwindcss/custom-forms": "^0.2.1",
    "eslint-config-google": "^0.12.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react-hooks": "^1.5.0",
    "gh-pages": "^2.0.1",
    "prettier": "^1.16.4",
    "tailwindcss": "^1.1.3"
  },
  "homepage": "https://phartenfeller.github.io/imdb-ratings-visualizer"
}
