{
  "name": "imdb-visualizer",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "apexcharts": "^3.5.1",
    "autoprefixer": "^9.7.6",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^7.1.1",
    "react": "^16.13.1",
    "react-apexcharts": "^1.3.0",
    "react-dom": "^16.13.1",
    "react-router-dom": "^5.1.2",
    "react-scripts": "^3.4.1",
    "react-table": "^7.0.0-beta.12",
    "victory": "^34.2.0"
  },
  "scripts": {
    "start": "npm-run-all --parallel watch:css start:react",
    "build": "npm-run-all build:css build:react",
    "build:css": "postcss src/styles/tailwind.css -o src/styles/index.css",
    "watch:css": "postcss src/styles/tailwind.css -o src/styles/index.css -w",
    "start:react": "react-scripts start",
    "build:react": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "eslint src",
    "deploy": "gh-pages -d build"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">1%",
    "not dead",
    "not IE > 0",
    "not op_mini all"
  ],
  "devDependencies": {
    "@tailwindcss/custom-forms": "^0.2.1",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^4.0.0",
    "gh-pages": "^2.0.1",
    "prettier": "^2.0.5",
    "tailwindcss": "^1.4.5"
  },
  "homepage": "https://phartenfeller.github.io/imdb-ratings-visualizer"
}
